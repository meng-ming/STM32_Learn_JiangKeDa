{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug STM32F407 (OpenOCD + ST-Link)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/WeatherClock.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "serverpath": "D:/ARM_Development_Toolchains/OpenOCD/xpack-openocd-0.12.0-7/bin/openocd.exe",
            "gdbPath": "D:/ARM_Development_Toolchains/arm-gnu-toolchain-13.2.Rel1/13.2.Rel1/bin/arm-none-eabi-gdb.exe",
            "device": "STM32F407ZG",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f4x.cfg"
            ],
            "svdFile": "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32F4xx/Source/Templates/STM32F407.svd",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw"
        }
    ]
}